<%= simple_form_for(@graduate_assistantship) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :division %>
    <%= f.input :department %>
    <%= f.input :description %>
    <%= f.input :gpa %>
    <%= f.input :infourl %>
    <%= f.input :applyurl %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
