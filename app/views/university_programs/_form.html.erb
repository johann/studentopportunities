<%= simple_form_for(@university_program) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :gpa %>
    <%= f.input :infourl %>
    <%= f.input :applyurl %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
