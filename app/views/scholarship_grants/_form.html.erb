<%= simple_form_for(@scholarship_grant) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :organization %>
    <%= f.input :scholarship %>
    <%= f.input :description %>
    <%= f.input :gpa %>
    <%= f.input :infourl %>
    <%= f.input :applyurl %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
